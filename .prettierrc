{
  "endOfLine": "lf",
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "types/(.*)$",
    "config/(.*)$",
    "lib/(.*)$",
    "hooks/(.*)$",
    "components/ui/(.*)$",
    "components/(.*)$",
    "^@uploadthing/(.*)$",
    "^uploadthing/(.*)$",
    "",
    "^~/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx"],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
